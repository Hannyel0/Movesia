{
  "name": "reactronite",
  "productName": "Reactronite",
  "version": "2.3.0",
  "description": "A modern, feature-rich Electron kit for building cross-platform desktop applications",
  "main": ".vite/build/main.js",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "type": "module",
  "keywords": [
    "electron",
    "react",
    "typescript",
    "boilerplate",
    "electron-forge",
    "vite"
  ],
  "author": {
    "name": "Flavio Del Grosso"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flaviodelgrosso/reactronite.git"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron"
    ],
    "ignoredBuiltDependencies": [
      "@tailwindcss/oxide",
      "better-sqlite3",
      "cpu-features",
      "electron-winstaller",
      "esbuild",
      "protobufjs",
      "ssh2",
      "unrs-resolver"
    ]
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development electron-forge start",
    "package": "cross-env NODE_ENV=production electron-forge package",
    "dev": "cross-env NODE_ENV=development electron-forge start",
    "make": "cross-env NODE_ENV=production electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "clean": "rimraf node_modules .vite pnpm-lock.yaml",
    "postinstall": "husky",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "release": "standard-version",
    "major": "npm run release -- --release-as major",
    "minor": "npm run release -- --release-as minor",
    "patch": "npm run release -- --release-as patch",
    "push-release": "git push --follow-tags origin master"
  },
  "dependencies": {
    "@langchain/core": "^0.3.66",
    "@langchain/langgraph-sdk": "^0.0.105",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "axios": "^1.11.0",
    "better-sqlite3": "^12.2.0",
    "chalk": "^4.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dockerode": "^4.0.7",
    "electron-squirrel-startup": "^1.0.1",
    "electron-window-state": "^5.0.3",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.523.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.2",
    "react-textarea-autosize": "^8.5.9",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.4",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.8.1",
    "@electron-forge/shared-types": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@electron/rebuild": "^4.0.1",
    "@tailwindcss/vite": "^4.1.10",
    "@types/better-sqlite3": "^7.6.13",
    "@types/dockerode": "^3.3.42",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "4.6.0",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "electron": "37.0.0",
    "electron-devtools-installer": "^4.0.0",
    "eslint": "^9.29.0",
    "husky": "9.1.7",
    "lint-staged": "^16.1.2",
    "neostandard": "^0.12.1",
    "pinst": "^3.0.0",
    "rimraf": "^6.0.1",
    "standard-version": "^9.5.0",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.1.10",
    "ts-node": "10.9.2",
    "typescript": "~5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-checker": "^0.9.3",
    "vite-plugin-svgr": "4.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
